<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
			location="/Users/zhaoyingdong/Utils/mysql-connector-java-8.0.12.jar" />
	<context id="context">
		<!-- 生成的pojo，将implements Serializable-->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
						connectionURL="jdbc:mysql://192.168.48.2:3306/agile?characterEncoding=UTF-8&amp;serverTimezone=Asia/Shanghai&amp;useSSL=false" userId="root"
						password="root+654321">
			<property name="remarksReporting" value="true" />
			<property name="useUnicode" value="true" />
			<property name="characterEncoding" value="UTF-8" />
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetPackage:包名；targetProject：生成文件的位置（相对于generatorConfig.xml所在项目，这里指的是agile-server）-->
		<javaModelGenerator
				targetPackage="com.yusys.agile.issue.domain"
				targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="./mybatis/mapper"
						 targetProject="./src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator
				targetPackage="com.yusys.agile.issue.dao"
				targetProject="./src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!--		<table tableName="s_team"/>-->
		<!--		<table tableName="s_team_user"/>-->
		<!--		<table tableName="s_team_system"/>-->
		<!--		<table tableName="s_team_po"/>-->
		<!--		<table tableName="s_team_sm"/>-->
		<!--		<table tableName="s_team_user"/>-->
		<!--		<table tableName="s_sprint"/>-->
		<table tableName="s_issue_richtext"/>

	</context>
</generatorConfiguration>
